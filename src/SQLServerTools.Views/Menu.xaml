<UserControl 
  x:Class="SQLServerTools.Views.Menu"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
  xmlns:local="clr-namespace:SQLServerTools.Views"
  xmlns:avalon="http://icsharpcode.net/sharpdevelop/avalonedit"
  xmlns:prism="http://prismlibrary.com/"
  mc:Ignorable="d" 

  d:DesignHeight="29" 
  d:DesignWidth="300">
<UserControl.Resources>
</UserControl.Resources>
<Grid>
  <Menu 
  FontSize="14"
  Height="29">
      <MenuItem Header="_File">
        <MenuItem Header="_Close Window" Command="{Binding CloseCommand}" CommandParameter="{Binding ElementName=MainWindow}"  />
      </MenuItem>
      <MenuItem Header="_Action">
        <MenuItem Header="_Table List" Command="{Binding AddTablePanel}"/>
      </MenuItem>
    </Menu>
</Grid>
</UserControl>

